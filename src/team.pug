include includes/header

// Navbar
body
  include includes/navbar
  
  .p-page
    main.l-main
      .p-cover-header.p-cover-header--team.js-cover-header
        h2.p-cover-header__title TEAM

      .container-fluid
        .p-block
          .p-title.p-title--center.text-center  
            h3.p-title__header #{member.header}
            p.p-title__body #{member.body}

          .p-member
            ul.row.p-member__list
              include mixin/team_member
              li.col-lg-3.col-xs-6.p-member__list-item(data-popup=member.profile.yasu.id)
                +team_member(member.profile.yasu.name, member.profile.yasu.role)(src='/img/pc/img_profile_yasu.png')
              li.col-lg-3.col-xs-6.p-member__list-item(data-popup=member.profile.mizusawa.id)
                +team_member(member.profile.mizusawa.name, member.profile.mizusawa.role)(src='/img/pc/img_profile_mizusawa.png')
              li.col-lg-3.col-xs-6.p-member__list-item(data-popup=member.profile.ishii.id)
                +team_member(member.profile.ishii.name, member.profile.ishii.role)(src='/img/pc/img_profile_ishii.png')
              li.col-lg-3.col-xs-6.p-member__list-item(data-popup=member.profile.usagi.id)
                +team_member(member.profile.usagi.name, member.profile.usagi.role)(src='/img/pc/img_profile_usagi.png')
              li.col-lg-3.col-xs-6.p-member__list-item(data-popup=member.profile.togashi.id)
                +team_member(member.profile.togashi.name, member.profile.togashi.role)(src='/img/pc/img_profile_togashi.png')
              li.col-lg-3.col-xs-6.p-member__list-item(data-popup=member.profile.kamei.id)
                +team_member(member.profile.kamei.name, member.profile.kamei.role)(src='/img/pc/img_profile_kamei.png')
              li.col-lg-3.col-xs-6.p-member__list-item(data-popup=member.profile.kubori.id)
                +team_member(member.profile.kubori.name, member.profile.kubori.role)(src='/img/pc/img_profile_kubori.png')

        .p-block--xs
          .p-title.p-title--center.text-center  
            h3.p-title__header #{ambassador.header}
            p.p-title__body #{ambassador.body}

          .p-ambassador
            include mixin/team_ambassador
            ul.p-ambassador__list
              li.row.p-ambassador__list-item
                .col-lg-6
                  span.p-ambassador__flag--japan
                  span.p-ambassador__country JAPAN
                .col-lg-6
                  .p-ambassador__profile
                    +team_ambassador(ambassador.profile.ricky.name, ambassador.profile.ricky.url)
              li.row.p-ambassador__list-item
                .col-lg-6
                .col-lg-6
                  .p-ambassador__profile
                    +team_ambassador(ambassador.profile.wine.name, ambassador.profile.wine.url)
              li.row.p-ambassador__list-item
                .col-lg-6
                .col-lg-6
                  .p-ambassador__profile
