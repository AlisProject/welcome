            ul.p-ambassador__list
              li.row.p-ambassador__list-item
                .col-lg-6
                  span.p-ambassador__flag--uk
                  span.p-ambassador__country UNITED KINGDOM
                .col-lg-6
                  .p-ambassador__profile
                    +team_ambassador(ambassador.profile.takuya.name, ambassador.profile.takuya.url)
            ul.p-ambassador__list
              li.row.p-ambassador__list-item
